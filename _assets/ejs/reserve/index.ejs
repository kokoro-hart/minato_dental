<% let pageId='reserve' ; %>

  <%- include('../common/_header', {pageId:pageId }) %>

    <% console.log(jsonData) %>
      <main>
        <!--メインビジュアル-->
        <section class="p-lower-mv">
          <picture class="p-lower-mv__picture">
            <source class="p-lower-mv__img" srcset="/img/webp/mv-contact_sp.webp" media="(max-width: 767px)" type="image/webp" />
            <source class="p-lower-mv__img" srcset="/img/webp/mv-contact_pc.webp" media="(min-width: 768px)" type="image/webp" />
            <source class="p-lower-mv__img" srcset="/img/common/mv-contact_sp.jpeg" media="(max-width: 767px)" />
            <img src="/img/common/mv-contact_pc.jpeg" class="p-lower-mv__img" media="(min-width: 768px)" decoding="async" alt="WEB予約">
          </picture>
          <div class="p-lower-mv__contents">
            <h2 class="p-lower-mv__heading">WEB予約</h2>
            <p class="p-lower-mv__lead">RESERVE</p>
          </div>
        </section>
        <!--/メインビジュアル-->

        <!--パンくずリスト-->
        <div class="c-breadcrumb u-mt10">
          <div class="l-inner">
            <ul class="c-breadcrumb__list" itemscope itemtype="https://schema.org/BreadcrumbList">
              <li class="c-breadcrumb__item" itemprop="itemListElement" itemscope
                itemtype="https://schema.org/ListItem">
                <a class="c-breadcrumb__link" itemprop="item" href="/"><span itemprop="name">ホーム</span></a>
                <meta itemprop="position" content="1" />
              </li>
              <li class="c-breadcrumb__item" itemprop="itemListElement" itemscope
                itemtype="https://schema.org/ListItem">
                <a class="c-breadcrumb__link" itemprop="item" href="/reserve/"><span itemprop="name">WEB予約</span></a>
                <meta itemprop="position" content="2" />
              </li>
            </ul>
          </div>
        </div>
        <!--/パンくずリスト-->

        <section class="p-contact l-lower">
          <div class="l-inner">
            <div class="p-contact-info">
              <h2 class="p-contact-info__title">お電話でのご予約/ご相談</h2>
              <address class="p-contact__address">
                <a href="tel:03-1234-5678" class="p-contact-info__tel">
                  <svg class="c-svg p-contact-info__tel-icon" width="28" height="28">
                    <use xlink:href="/img/svg/sprite.min.svg#icon-tel" />
                  </svg>
                  03-1234-5678
                  <p class="p-contact-info__time">(年中無休 AM9:00〜PM22:00)</p>
                </a>
              </address>
              <p class="p-contact-info__text">
                お急ぎの方は電話での連絡がスムーズです。<br>
                混雑状況によっては当日受診をご利用いただけない場合がございます。<br>
                あらかじめご了承ください。
              </p>
              <h2 class="p-contact-info__title">メールでのご予約/ご相談</h2>
              <p class="p-contact-info__text">
                【ご予約に関しての注意点】<br>
                メールアドレスの入力間違いにより送信できない事が発生しておりますので、メールアドレスは正しくご入力下さい。<br>
                ※24時間以内に当院からの返信がない場合には、お電話(<a href="tel:0312345678">TEL03-1234-5678</a>)にてお問い合わせ下さい。
              </p>
            </div>
            <div class="p-contact__title-wrapper">
              <h2 class="c-section-title">予約フォーム</h2>
            </div>
            <div class="l-lower-form">
              <!--コンタクトフォーム-->
              <div class="p-form">
                <dl class="p-form__dl">
                  <!--お名前-->
                  <div class="p-form__row">
                    <dt class="p-form__dt">
                      <label class="p-form__label p-form__label--required" for="your-name">お名前</label>
                    </dt>
                    <dd class="p-form__dd">
                      <input class="p-form__input" id="your-name" type="text" placeholder="山田　太郎" autocomplete="name" required>
                    </dd>
                  </div>
                  <!--/お名前-->
        
                  <!--フリガナ-->
                  <div class="p-form__row">
                    <dt class="p-form__dt">
                      <label class="p-form__label p-form__label--required" for="your-ruby">フリガナ</label>
                    </dt>
                    <dd class="p-form__dd">
                      <input class="p-form__input" id="your-ruby" type="text" placeholder="ヤマダ　タロウ" autocomplete="off" required>
                    </dd>
                  </div>
                  <!--/フリガナ-->
        
                  <!--電話番号-->
                  <div class="p-form__row">
                    <dt class="p-form__dt">
                      <label class="p-form__label p-form__label--required" for="your-tel">電話番号</label>
                    </dt>
                    <dd class="p-form__dd">
                      <input class="p-form__input" id="your-tel" type="text" placeholder="000-0000-0000" autocomplete="tel"
                        required>
                    </dd>
                  </div>
                  <!--/電話番号-->
        
                  <!--メールアドレス-->
                  <div class="p-form__row">
                    <dt class="p-form__dt">
                      <label class="p-form__label p-form__label--required" for="your-email">メールアドレス</label>
                    </dt>
                    <dd class="p-form__dd">
                      <input class="p-form__input" id="your-email" type="email" placeholder="xxx@example.com"
                        autocomplete="email" required>
                    </dd>
                  </div>
                  <!--/メールアドレス-->
        
                  <!--初診/再診-->
                  <div class="p-form__row">
                    <dt class="p-form__dt">
                      <label for="your-radio" class="p-form__label p-form__label--required">初診/再診</label>
                    </dt>
                    <dd class="p-form__dd">
                      <label>
                        <input class="p-form__radio" name="q1" type="radio" id="your-radio" value="初診" required checked>
                        <span class="p-form__radio-part">初診</span>
                      </label>
                      <label>
                        <input class="p-form__radio" name="q1" type="radio" id="your-radio" value="再診" required>
                        <span class="p-form__radio-part">再診</span>
                      </label>
                    </dd>
                  </div>
                  <!--/初診/再診-->

                  <!--診察内容-->
                  <div class="p-form__row">
                    <dt class="p-form__dt">
                      <label class="p-form__label p-form__label--required">診察内容 <div class="u-color-gray u-fz11">※(複数選択可)</div></label>
                    </dt>
                    <dd class="p-form__dd">
                      <span class="checkbox-1">
                        <span>
                          <label>
                            <input type="checkbox" name="checkbox-1" value="虫歯">
                            <span>虫歯</span>
                          </label>
                        </span>
                        <span>
                          <label>
                            <input type="checkbox" name="checkbox-1" value="虫歯">
                            <span>被せ物がとれた</span>
                          </label>
                        </span>
                        <span>
                          <label>
                            <input type="checkbox" name="checkbox-1" value="矯正歯科">
                            <span>矯正歯科</span>
                          </label>
                        </span>
                        <span>
                          <label>
                            <input type="checkbox" name="checkbox-1" value="咬み合わせ">
                            <span>咬み合わせ</span>
                          </label>
                        </span>
                        <span>
                          <label>
                            <input type="checkbox" name="checkbox-1" value="歯周病">
                            <span>歯周病</span>
                          </label>
                        </span>
                        <span>
                          <label>
                            <input type="checkbox" name="checkbox-1" value="小児歯科">
                            <span>小児歯科</span>
                          </label>
                        </span>
                        <span>
                          <label>
                            <input type="checkbox" name="checkbox-1" value="入れ歯">
                            <span>入れ歯</span>
                          </label>
                        </span>
                        <span>
                          <label>
                            <input type="checkbox" name="checkbox-1" value="インプラント">
                            <span>インプラント</span>
                          </label>
                        </span>
                        <span>
                          <label>
                            <input type="checkbox" name="checkbox-1" value="その他">
                            <span>その他</span>
                          </label>
                        </span>
                      </span>
                    </dd>
                  </div>
                  <!--/診察内容-->

                  <!--ご連絡方法-->
                  <div class="p-form__row">
                    <dt class="p-form__dt">
                      <label class="p-form__label" for="your-select">ご連絡方法</label>
                    </dt>
                    <dd class="p-form__dd">
                      <select id="your-select" class="p-form__input p-form__select" required>
                        <option value="" selected>メール</option>
                        <option value="option1">電話</option>
                        <option value="option2">その他</option>
                      </select>
                    </dd>
                  </div>
                  <!--/ご連絡方法-->

                  <!--希望日-->
                  <div class="p-form__row">
                    <dt class="p-form__dt">
                      <label class="p-form__label" for="your-reserve">希望日</label>
                    </dt>
                    <dd class="p-form__dd">
                      <span class="date-1">
                        <input class="p-form__input p-form__date" type="date"  id="your-reserve" name="reserve" placeholder="第一希望">
                      </span>
                      <span class="date-2">
                        <input class="p-form__input p-form__date" type="date" id="your-reserve" name="reserve" placeholder="第二希望">
                      </span>
                      <span class="date-3">
                        <input class="p-form__input p-form__date" type="date" id="your-reserve" name="reserve" placeholder="第三希望">
                      </span>
                    </dd>
                  </div>
                  <!--/希望日-->

                  <!--お問い合わせ内容-->
                  <div class="p-form__row">
                    <dt class="p-form__dt">
                      <label class="p-form__label" for="your-textarea">お問い合わせ内容</label>
                    </dt>
                    <dd class="p-form__dd">
                      <textarea class="p-form__input p-form__textarea" id="your-textarea" placeholder="ご自由にご記入ください。"></textarea>
                    </dd>
                  </div>
                  <!--/お問い合わせ内容-->

                </dl>
        
                <div class="p-form__submit-wrapper">
                  <input type="submit" class="c-button-submit" value="送信">
                </div>
              </div>
              <!--/コンタクトフォーム-->
            </div>
          </div>
        </section>


      </main>

      <%- include('../common/_footer', {pageId:pageId }) %>